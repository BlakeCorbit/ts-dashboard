# Zendesk API
ZENDESK_SUBDOMAIN=bayiq
ZENDESK_EMAIL=your-email@autovitalsinc.com
ZENDESK_API_TOKEN=your-zendesk-api-token

# Slack
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL

# Clustering
CLUSTER_THRESHOLD=3
CLUSTER_WINDOW_MINUTES=15
POLL_INTERVAL_SECONDS=60

# Keywords that indicate POS-related issues (comma-separated)
POS_KEYWORDS=RO not showing,repair order,not syncing,data not,integration,binary,no data,missing RO,TVP empty,not transferring

# Keywords that indicate email/SMS issues
COMMS_KEYWORDS=email not sending,mailgun,twilio,text message,SMS,not receiving texts,reminder not sent

# Keywords that indicate login/access issues
ACCESS_KEYWORDS=welcome code,login,password,can't access,locked out,app not working
